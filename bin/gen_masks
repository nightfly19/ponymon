#!/usr/bin/env bash
echo "Generating xor masks"
result=0
for i in original/*;do
    echo $i
    cat "working/$(basename "$i")" | ./bin/xor_file "$i" | base64 - > "patch_mask/$(basename "$i")"
    if [ $? -eq 0 ];then
        echo -e "\t[OK]";
    else
        echo -e "\t[failed]";
        result=0
    fi
done

exit $result
