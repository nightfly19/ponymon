#!/usr/bin/env bash
echo "Applying xor masks"
result=0
for i in original/*;do
    echo $i
    ./bin/xor_files "$i" "patch_mask/$(basename "$i")" "working/$(basename "$i")"
    if [ $? -eq 0 ];then
        echo -e "\t[OK]";
    else
        echo -e "\t[failed]";
        result=0
    fi
done

exit $result
